<?xml version="1.0" encoding="UTF-8"?>

<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<pluginGroups>
		<pluginGroup>com.link_intersystems.maven.plugins</pluginGroup>
	</pluginGroups>

	<proxies />

	<servers>
		<server>
			<id>https://p2.link-intersystems.com/comercial/lis-eclipse/latest</id>
			<username>${env.P2_USER}</username>
			<password>${env.P2_PASS}</password>
		</server>
		<server>
			<id>p2</id>
			<username>${env.P2_USER}</username>
			<password>${env.P2_PASS}</password>
			<configuration>
				<strictHostKeyChecking>false</strictHostKeyChecking>
			</configuration>
		</server>
		<server>
			<id>https://nexus.link-intersystems.com/repository/eclipse-updates-4.15</id>
			<username>${env.NEXUS_USER}</username>
			<password>${env.NEXUS_PASS}</password>
		</server>
		<server>
			<id>https://nexus.link-intersystems.com/repository/eclipse-2020-03</id>
			<username>${env.NEXUS_USER}</username>
			<password>${env.NEXUS_PASS}</password>
		</server>
		<server>
			<id>https://nexus.link-intersystems.com/repository/eclipse-nebula-2.3.0</id>
			<username>${env.NEXUS_USER}</username>
			<password>${env.NEXUS_PASS}</password>
		</server>
		<server>
			<id>link-intersystems-releases</id>
			<username>${env.NEXUS_USER}</username>
			<password>${env.NEXUS_PASS}</password>
		</server>
		<server>
			<id>link-intersystems-snapshots</id>
			<username>${env.NEXUS_USER}</username>
			<password>${env.NEXUS_PASS}</password>
		</server>

		<server>
			<id>link-intersystems-plugin-releases</id>
			<username>${env.NEXUS_USER}</username>
			<password>${env.NEXUS_PASS}</password>
		</server>
		<server>
			<id>link-intersystems-plugin-snapshots</id>
			<username>${env.NEXUS_USER}</username>
			<password>${env.NEXUS_PASS}</password>
		</server>
	</servers>

	<mirrors>
		<mirror>
			<id>link-intersystems-releases</id>
			<mirrorOf>central</mirrorOf>
			<name>Link Intersystems Nexus</name>
			<url>https://nexus.link-intersystems.com/repository/releases</url>
		</mirror>
	</mirrors>


	<profiles>
		<profile>
			<id>link-intersystems-releases-profile</id>

			<repositories>
				<repository>
					<id>link-intersystems-releases</id>
					<name>Link Intersystems Releases</name>
					<url>https://nexus.link-intersystems.com/repository/releases/</url>
					<layout>default</layout>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
					<snapshots>
						<enabled>false</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>fail</checksumPolicy>
					</snapshots>
				</repository>
				<repository>
					<id>link-intersystems-snapshots</id>
					<name>Link Intersystems Snapshots</name>
					<url>https://nexus.link-intersystems.com/repository/snapshots/</url>
					<layout>default</layout>
					<releases>
						<enabled>false</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>fail</checksumPolicy>
					</snapshots>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>link-intersystems-plugin-releases</id>
					<name>Link Intersystems Plugin Releases</name>
					<url>https://nexus.link-intersystems.com/repository/releases/</url>
					<layout>default</layout>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
					<snapshots>
						<enabled>false</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>fail</checksumPolicy>
					</snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>link-intersystems-plugin-snapshots</id>
					<name>Link Intersystems Plugin Snapshots</name>
					<url>https://nexus.link-intersystems.com/repository/snapshots/</url>
					<layout>default</layout>
					<releases>
						<enabled>false</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>fail</checksumPolicy>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>

	<activeProfiles>
		<activeProfile>link-intersystems-releases-profile</activeProfile>
	</activeProfiles>
</settings>

